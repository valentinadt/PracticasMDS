library(ggplot2)
library( nycflights13)
data('flights')
#data('airports')
attach (flights)
data('airports')
attach (airports)
ggplot(flights,aes(x = month))+geom_bar()
ggplot(flights, aes(x=dep_delay, y=arr_delay) ) +
geom_density_2d()
ggplot(airports, aes(lon,lat)) + geom_point(aes(size=alt),alpha= 0.05)
